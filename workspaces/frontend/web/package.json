{
  "name": "@app/web",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "type": "module",
  "scripts": {
    "prebuild": "npm run clean",
    "build": "vite build",
    "clean": "rimraf ./dist",
    "codegen": "graphql-codegen && prettier --write ./src/gql-client/types",
    "format": "prettier --write ./",
    "lint": "npm run lint:eslint && npm run lint:format && npm run lint:circular",
    "lint:circular": "dpdm -T --exit-code circular:1 --no-warning --no-tree ./src/index.tsx",
    "lint:eslint": "eslint --max-warnings 0",
    "lint:format": "prettier --check .",
    "start": "vite preview",
    "start:dev": "vite",
    "test": "tsc --noEmit"
  },
  "dependencies": {
    "@app/common": "1.0.0",
    "@graphql-codegen/add": "5.0.3",
    "@solidjs/router": "^0.15.0",
    "@urql/core": "^5.0.8",
    "bootstrap": "5.3.3",
    "decode-formdata": "0.8.0",
    "effect": "3.10.16",
    "graphql": "^16.9.0",
    "graphql-ws": "^5.16.0",
    "pretty-bytes": "6.1.1",
    "solid-js": "^1.8.11",
    "walkjs": "6.0.1",
    "wonka": "6.3.4"
  },
  "devDependencies": {
    "@app/tsconfig": "1.0.0",
    "@eslint/js": "^9.14.0",
    "@graphql-codegen/cli": "^5.0.3",
    "@graphql-codegen/client-preset": "^4.5.0",
    "@types/bootstrap": "^5.2.10",
    "@types/eslint__js": "^8.42.3",
    "dpdm": "^3.14.0",
    "eslint": "^9.14.0",
    "eslint-plugin-solid": "^0.14.4",
    "rimraf": "^6.0.1",
    "sass-embedded": "1.81.0",
    "typescript-eslint": "^8.12.2",
    "vite": "^5.0.11",
    "vite-plugin-solid": "^2.8.2"
  }
}
